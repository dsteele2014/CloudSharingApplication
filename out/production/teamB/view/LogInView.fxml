<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="293.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LogInViewController">
   <children>
      <Group layoutX="50.0" layoutY="133.0">
         <children>
            <Label prefHeight="27.0" prefWidth="112.0" text="User Name: ">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="" layoutX="140.0" layoutY="2.0" />
         </children>
      </Group>
      <Group layoutX="52.0" layoutY="183.0">
         <children>
            <PasswordField fx:id="inputPassword" layoutX="135.0" layoutY="1.0" />
            <Label text="Password: ">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </Group>
      <Button layoutX="173.0" layoutY="231.0" mnemonicParsing="false" onAction="#handleConfirmButton" text="Confirm" />
      <Label contentDisplay="CENTER" layoutX="141.0" layoutY="33.0" prefHeight="80.0" prefWidth="119.0" text="Log In">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleBackButton" prefHeight="35.0" prefWidth="52.0" text="Back">
